	"version": "1.0.0",
	"type": "library",
	"keywords": ["content", "brilliant"],
	"homepage": "https://github.com/konservs/brilliant.content",
	"license": "MIT",
	"minimum-stability": "dev",
	"authors": [
		{
		"name": "Andrii Biriev",
		"email": "andrii.biriev@gmail.com",
		"homepage": "https://konservs.com",
		"role": "Developer"
		}
	],
	"support": {
		"email": "andrii.biriev@gmail.com"
	},
	"require": {
		"konservs/brilliant.framework": "*"
	},
	"autoload": {
		"psr-4": {
			"Brilliant\\Content\\": "libraries/Content"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php bin/PostInstall.php"
		],
		"post-update-cmd": [
			"php bin/PostUpdate.php"
		]
	}
}
